{
  "name": "icpc-eelu-landing-page",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "biome:check": "biome check .",
    "biome:format": "biome format --write .",
    "dprint": "dprint fmt",
    "dprint:check": "dprint check",
    "typecheck": "tsc --noEmit",
    "clean:dist": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "clean:locks": "rimraf pnpm-lock.yaml package-lock.json yarn.lock",
    "clean:caches": "rimraf .turbo .parcel-cache .cache",
    "clean": "pnpm run clean:dist && pnpm run clean:caches",
    "reset": "pnpm run clean && pnpm run clean:modules && pnpm run clean:locks",
    "check:all": "pnpm run lint && pnpm run format:check && pnpm run dprint:check && pnpm run biome:check && pnpm run typecheck",
    "format:all": "pnpm run lint:fix && pnpm run format && pnpm run dprint && pnpm run biome:format",
    "prepare": ""
  },
  "dependencies": {
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/ts-plugin": "^1.10.4",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@eslint/js": "^9.35.0",
    "astro-eslint-parser": "^1.2.2",
    "dprint": "^0.50.1",
    "eslint": "^9.35.0",
    "eslint-config-xo": "^0.49.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
