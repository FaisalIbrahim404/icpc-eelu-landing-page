{
  "name": "icpc-eelu-landing-page",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "lint:astro": "eslint \"**/*.astro\"",
    "lint": "pnpm run lint:astro",
    "prettier:write": "prettier --write \"**/*.{astro,css}\"",
    "prettier:check": "prettier --check \"**/*.{astro,css}\"",
    "biome:check": "biome check --reporter=summary",
    "biome:format": "biome format --write",
    "dprint:check": "dprint check",
    "dprint:write": "dprint fmt",
    "typecheck": "tsc --noEmit",
    "check:all": "pnpm run lint && pnpm run prettier:check && pnpm run biome:check && pnpm run dprint:check && pnpm run typecheck",
    "format:all": "pnpm run prettier:write && pnpm run biome:format && pnpm run dprint:write",
    "clean:dist": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "clean:locks": "rimraf pnpm-lock.yaml package-lock.json yarn.lock",
    "clean:caches": "rimraf .turbo .parcel-cache .cache",
    "clean": "pnpm run clean:dist && pnpm run clean:caches",
    "reset": "pnpm run clean && pnpm run clean:locks && pnpm run clean:modules",
    "deps:unused": "depcheck",
    "deps:review": "NPM_CHECK_INSTALLER=pnpm npm-check",
    "deps:update:interactive": "NPM_CHECK_INSTALLER=pnpm npm-check -u"
  },
  "dependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@gsap/react": "^2.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.543.0",
    "ogl": "^1.0.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.3.1",
    "@biomejs/biome": "2.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.5",
    "astro-eslint-parser": "^1.2.2",
    "dprint": "^0.50.1",
    "eslint": "^9.35.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
